  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@abstract-foundation/agw-client": "^1.9.0",
    "@abstract-foundation/agw-react": "^1.9.0",
    "@tanstack/react-query": "^5.85.5",
    "@types/dompurify": "^3.2.0",
    "alchemy-sdk": "^3.6.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.2.6",
    "lucide-react": "^0.541.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.1",
    "viem": "^2.36.0",
    "wagmi": "^2.16.9"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "plasmo": "^0.90.5",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "typescript": "^5.9.2"
  },
  "manifest": {
    "host_permissions": [
      "https://api.opensea.io/*",
      "https://eth-mainnet.g.alchemy.com/*",
      "https://abstract-mainnet.g.alchemy.com/*",
      "https://gateway.pinata.cloud/*",
      "https://ipfs.io/*",
      "https://cloudflare-ipfs.com/*",
      "https://i.seadn.io/*",
      "https://openseauserdata.com/*",
      "https://api.testnet.abs.xyz/*",
      "https://explorer.testnet.abs.xyz/*",
      "https://*.x.com/*",
      "https://*.twitter.com/*"
    ],
    "permissions": [
      "tabs",
      "scripting",
      "activeTab",
      "storage"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'; img-src 'self' https://*.opensea.io https://eth-mainnet.g.alchemy.com https://abstract-mainnet.g.alchemy.com https://*.pinata.cloud https://ipfs.io https://cloudflare-ipfs.com https://i.seadn.io https://openseauserdata.com data:;"
    }
  }
}
